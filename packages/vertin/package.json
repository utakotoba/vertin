{
  "name": "vertin",
  "type": "module",
  "version": "0.3.1-dev",
  "description": "A universal command line app helper",
  "author": "Akiyama Kohane <echo.aimless@gmail.com>",
  "license": "MPL-2.0",
  "homepage": "https://github.com/utakotoba/vertin#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/utakotoba/vertin.git",
    "directory": "packages/vertin"
  },
  "keywords": ["cli", "args"],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./parser": {
      "types": "./dist/parser/index.d.ts",
      "import": "./dist/parser/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "LICENSE",
    "dist"
  ],
  "scripts": {
    "build": "rolldown -c rolldown.config.ts",
    "prepack": "bun run build"
  }
}
